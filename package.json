{
  "name": "nuxt-electron-starter",
  "version": "1.0.0",
  "description": "nuxt-electron-starter",
  "main": "dist/main/index.js",
  "scripts": {
    "dev": "npm-run-all -p dev:main dev:electron",
    "dev:main": "cross-env NODE_ENV=development tsc -p src/main/tsconfig.json",
    "dev:electron": "cross-env NODE_ENV=development electron .",
    "nuxt": "cross-env NODE_ENV=development nuxt src/renderer",
    "build": "npm-run-all build:main build:renderer build:electron -- {@}",
    "build:main": "cross-env NODE_ENV=production tsc -p src/main/tsconfig.json",
    "build:renderer": "cross-env NODE_ENV=production nuxt generate src/renderer",
    "build:electron": "cross-env NODE_ENV=production electron-builder",
    "build:all": "npm-run-all build -- -mwl",
    "build:win": "npm-run-all build -- --win",
    "build:mac": "npm-run-all build -- --mac",
    "build:linux": "npm-run-all build -- --linux",
    "lint": "npm-run-all lint:js lint:style lint:prettier",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{css,scss,sass,html,vue}\" --ignore-path .gitignore",
    "lint:prettier": "prettier --write --check .",
    "clean": "npm-run-all clean:build clean:dist",
    "clean:build": "rimraf build/*",
    "clean:dist": "rimraf dist/*"
  },
  "author": "miyabin66",
  "license": "UNLICENSED",
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "core-js": "^3.19.3",
    "nuxt": "^2.15.8",
    "reset-css": "^5.0.1",
    "vue": "^2.6.14",
    "vue-server-renderer": "^2.6.14",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^4.46.0"
  },
  "devDependencies": {
    "@aceforth/nuxt-optimized-images": "^1.4.0",
    "@babel/eslint-parser": "^7.16.3",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^8.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/google-analytics": "^2.4.0",
    "@nuxtjs/style-resources": "^1.2.1",
    "@nuxtjs/stylelint-module": "^4.1.0",
    "@types/sass": "^1.43.1",
    "cross-env": "^7.0.3",
    "electron": "^18.1.0",
    "electron-builder": "^23.0.3",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^3.1.0",
    "eslint-plugin-vue": "^8.2.0",
    "eslint-plugin-vuejs-accessibility": "^1.1.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "nuxt-compress": "^5.0.0",
    "postcss-html": "^1.3.0",
    "prettier": "^2.5.1",
    "pug": "^3.0.2",
    "pug-loader": "^2.4.0",
    "pug-plain-loader": "^1.1.0",
    "rimraf": "^3.0.2",
    "sass": "^1.49.9",
    "sass-loader": "^10.1.1",
    "stylelint": "^14.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.1.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-scss": "^4.1.0",
    "typescript": "4.2.4",
    "webp-loader": "^0.6.0"
  }
}
